{
    "extends": "./node_modules/gts/tsconfig-google",
    "compilerOptions": {
        "target": "es6",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noImplicitUseStrict": false,
        "removeComments": true,
        "importHelpers": true,
        "esModuleInterop": true,
        "noEmitHelpers": true,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "jsx": "react",
        "newLine": "lf",
        "stripInternal": true,
        "skipLibCheck": true,
        "noErrorTruncation": true,
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "composite": true,
        "incremental": true
    }
}