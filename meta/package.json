    "@types/rimraf": "^3.0.0",
    "concurrently": "^5.1.0",
    "fuse-box": "^4.0.0-next.432",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "tsc-watch": "^4.2.5"
  },
  "dependencies": {
    "@cleric/common": "0.0.9",
    "jsonschema": "^1.2.5",
    "quicktype": "^15.0.210",
    "ramda": "^0.27.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "ts-toolbelt": "^6.7.7"
  },
  "types": "dist/index.ts"
